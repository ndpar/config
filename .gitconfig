[user]
    name = Andrey Paramonov
    email = git@ndpar.com
[color]
    ui = auto
[alias]
    st = status
    ci = commit
    br = branch
    co = checkout
    df = diff
    sb = show-branch
    lg = log -p
    k = log --graph --pretty=format:'%Cred%h%Creset%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev=8
[rerere]
    enabled = 1
